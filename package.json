    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chokidar": "^2.0.4",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
