{
  "name": "webpack-config",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/nameless19922/webpack-config.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "nodemon -L",
    "build": "rimraf dist && webpack --mode production",
    "lint": "eslint ./app --ext .js,"
  },
  "nodemonConfig": {
    "exec": "babel-node index.js",
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "server/client.js"
    ],
    "verbose": true,
    "watch": [
      "webpack.config.js",
      "index.js",
      "config",
      "server"
    ],
    "ext": "js json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
