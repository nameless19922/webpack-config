    "nodemon": "^1.18.7",
    "nunjucks-html-loader": "^1.1.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
