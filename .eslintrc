{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "jquery": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "class-methods-use-this": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "global-require": 0
  }
}
